{
  "manifest_version": 3,
  "name": "MSF ISO-8 Counter/Assist Viewer",
  "version": "1.8.2",
  "description": "Displays ISO-8 Counter Attack/Assist ability information on Marvel Strike Force character pages",
  "permissions": [],
  "host_permissions": [
    "https://marvelstrikeforce.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://marvelstrikeforce.com/en/characters/*"],
      "js": ["iso8_data.js", "content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
